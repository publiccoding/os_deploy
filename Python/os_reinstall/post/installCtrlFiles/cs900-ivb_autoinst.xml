        <interpreter>shell</interpreter>
        <filename>cs900-autoinst-pre-script.sh</filename>
        <source><![CDATA[
#!/bin/bash

# Input parameter
OS_DEVICE_LUN_ID=4

# External programs used
CAT=/usr/bin/cat
CP=/bin/cp
DMIDECODE=/usr/sbin/dmidecode
ECHO=/usr/bin/echo
FIND=/usr/bin/find
SED=/usr/bin/sed

# Result of steps and script
result=0

# Set and clear log file
LOG="/tmp/cs900-autoinst-pre-script.log"
>$LOG

# Print command line arguments 
$ECHO "arguments=$0 $@" >>$LOG

# Copy file medium.id from the installation medium
$CP -p /var/adm/mount/*/medium.id /tmp/medium.id

# Inital copy of /tmp/profile/autoinst.xml to /tmp/profile/modified.xml
$CP -p /tmp/profile/autoinst.xml /tmp/profile/modified.xml

# Check if the manufacturer and product name is supported
system_manufacturer=$($DMIDECODE -s system-manufacturer)
$ECHO "system_manufacturer=$system_manufacturer" >>$LOG
system_product_name=$($DMIDECODE -s system-product-name)
$ECHO "system_product_name=$system_product_name" >>$LOG
case "$system_manufacturer" in
    *"HP"* | *"HPE"*)
        case "$system_product_name" in
        *"Superdome2"*)
            append="splash=silent quiet noresume showopts log_buf_len=2M intel_idle.max_cstate=0 processor.max_cstate=1 elevator=noop biosdevname=1 console=ttyS0,115200"
            append_failsafe="noresume showopts apm=off edd=off powersaved=off nohz=off highres=off processor.max_cstate=1 nomodeset x11failsafe console=ttyS0,115200"
            kdump_memory_low="256M,low"
            kdump_memory_high="768M,high"
#            device_pattern_match="^/dev/disk/by-id/scsi\-[0-9a-f]+.$"
            device_pattern_match="^/dev/disk/by-id/wwn\-0x[0-9a-f]+.$"
            device_lun_id_position=42
            device_lun_id_length=4
            ;;
        *"ProLiant DL360 Gen9"*)
            append="splash=silent quiet noresume showopts log_buf_len=2M intel_idle.max_cstate=0 processor.max_cstate=1 elevator=noop biosdevname=1"
            append_failsafe="noresume showopts apm=off edd=off powersaved=off nohz=off highres=off processor.max_cstate=1 nomodeset x11failsafe"
            kdump_memory_low="128M,low"
            kdump_memory_high="256M,high"
#            device_pattern_match="^/dev/disk/by-id/scsi\-[0-9a-f]+.$"
            device_pattern_match="^/dev/disk/by-id/wwn\-0x[0-9a-f]+.$"
            device_lun_id_position=42
            device_lun_id_length=4
            ;;
        *)
            $ECHO "Error: Unsupported product name: $system_product_name"
            $ECHO "Error: Unsupported product name: $system_product_name" >>$LOG
            result = 1
            ;;
        esac
    ;;
    *"VMware"*)
        append="splash=silent quiet noresume showopts log_buf_len=2M intel_idle.max_cstate=0 processor.max_cstate=1 elevator=noop biosdevname=1"
        append_failsafe="noresume showopts apm=off edd=off powersaved=off nohz=off highres=off processor.max_cstate=1 nomodeset x11failsafe"
        kdump_memory_low="128M,low"
        kdump_memory_high="256M,high"
#        device_pattern_match="^/dev/disk/by-id/scsi\-VMware_Virtual_SATA_Hard_Drive_[0-9]+$"
        device_pattern_match="^/dev/disk/by-id/wwn\-0xMware_Virtual_SATA_Hard_Drive_[0-9]+$"
        device_lun_id_position=52
        device_lun_id_length=2
    ;;
    *)
        $ECHO "Error: Unsupported manufacturer: $system_manufacturer"
        $ECHO "Error: Unsupported manufacturer: $system_manufacturer" >>$LOG
        result = 1
    ;;
esac

if [[ $result = 0 ]]; then
    # Set "append" and "append_failsafe" entries in bootloader section
    $SED -i "s|^\([ \t]*<append>\)APPEND\(</append>\)|\1$append\2|" /tmp/profile/modified.xml
    $SED -i "s|^\([ \t]*<append_failsafe>\)APPEND_FAILSAFE\(</append_failsafe>\)|\1$append_failsafe\2|" /tmp/profile/modified.xml
    # Set KDUMP low and high memory allocation in kdump section
    $SED -i "s|^\([ \t]*<listentry>\)KDUMP_MEMORY_LOW\(</listentry>\)|\1$kdump_memory_low\2|" /tmp/profile/modified.xml
    $SED -i "s|^\([ \t]*<listentry>\)KDUMP_MEMORY_HIGH\(</listentry>\)|\1$kdump_memory_high\2|" /tmp/profile/modified.xml
fi

if [[ $result = 0 ]]; then
    # Check the list of devices for a match of OS_DEVICE_LUN_ID
    devices=($($FIND /dev/disk/by-id/ -regex $device_pattern_match))
    devices_matched=0
    for device in ${devices[@]}; do
        device_lun_id=$((16#${device:$device_lun_id_position:$device_lun_id_length}))
        $ECHO "device_lun_id=$device_lun_id, device=$device" >>$LOG
        if [[ $device_lun_id -eq $OS_DEVICE_LUN_ID ]]; then
            devices_matched=$(($devices_matched + 1))
            os_device=$device
            $ECHO "os_device=$os_device" >>$LOG
        fi
    done
    if [[ $devices_matched = 0 ]]; then
        $ECHO "Error: No device with LUN ID $OS_DEVICE_LUN_ID found"
        $ECHO "Error: No device with LUN ID $OS_DEVICE_LUN_ID found" >>$LOG
        result=1
    elif [[ $devices_matched > 1 ]]; then
        $ECHO "Error: Multiple devices with LUN ID $OS_DEVICE_LUN_ID found"
        $ECHO "Error: Multiple devices with LUN ID $OS_DEVICE_LUN_ID found" >>$LOG
        result=1
    fi
fi

if [[ $result = 0 ]]; then
    # Insert OS device into /tmp/profile/modified.xml
    $SED -i "s|^\([ \t]*<device>\)DEVICE\(</device>\)|\1$os_device\2|" /tmp/profile/modified.xml
    $SED -i "s|^\([ \t]*<linux>\)LINUX\(</linux>\)|\1$os_device\2|" /tmp/profile/modified.xml
    # Check if OS device has partitions configured
    partitions=($($FIND $os_device*))
    partition_count=${#partitions[@]}
    if [[ $partition_count != 1 ]]; then
        $ECHO "Warning: Device with LUN ID $OS_DEVICE_LUN_ID has partitions" >>$LOG
        result=2
    fi
fi

cmdline=$($CAT /proc/cmdline)
if [[ $result = 2 ]]; then
    # Change <general><ask-list><ask> section to "Overwrite"  
    $SED -i "s|^\([ \t]*<title>\)TITLE\(</title>\)|\1Overwrite device $os_device?\2|" /tmp/profile/modified.xml
    $SED -i "s|^\([ \t]*<label>\)LABEL\(</label>\)|\1Overwrite and continue installation\2|" /tmp/profile/modified.xml
    $SED -i "s|^\([ \t]*<default>\)DEFAULT\(</default>\)|\1continue\2|" /tmp/profile/modified.xml
elif [[ $result = 1 || $cmdline = *"_CS900_DEBUG"* ]]; then
    # Change <general><ask-list><ask> section to "Debug"
    $SED -i "s|^\([ \t]*<title>\)TITLE\(</title>\)|\1Debug pre-install script\2|" /tmp/profile/modified.xml
    $SED -i "s|^\([ \t]*<label>\)LABEL\(</label>\)|\1Continue installation\2|" /tmp/profile/modified.xml
    $SED -i "s|^\([ \t]*<default>\)DEFAULT\(</default>\)|\1display\2|" /tmp/profile/modified.xml
elif [[ $result = 0 ]]; then
    # Remove <general><ask-list> section as no interaction is needed
    $SED -i "\|^[ \t]*<ask-list config:type=\"list\">|,\|^[ \t]*</ask-list>|d" /tmp/profile/modified.xml
fi

$CAT /tmp/profile/modified.xml >>$LOG
$ECHO "result=$result" >>$LOG
exit $result
        ]]></source>
      </script>
