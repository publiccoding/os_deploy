        <source><![CDATA[
#!/bin/bash

# AutoYaST will re-direct stdout/stderr to /var/adm/autoinstall/logs/cs900-autoinst-chroot-script.sh.log

# External programs used
AWK=/usr/bin/awk
CAT=/usr/bin/cat
CP=/bin/cp
ECHO=/usr/bin/echo
MV=/usr/bin/mv

# Result of steps and script
result=0

# Save cs900-autoinst-pre-script.sh log file to persistent storage
$ECHO "# Save cs900-autoinst-pre-script.sh log file to persistent storage"
$MV /tmp/cs900-autoinst-pre-script.log /mnt/var/adm/autoinstall/logs

# Clear udev persistent net rules
$ECHO "# Clear udev persistent net rules"
>/mnt/etc/udev/rules.d/70-persistent-net.rules

# Create udev rule for iLO virtual folder and DVD
$ECHO "# Create udev rule for iLO virtual folder and DVD"
$CAT <<EOF >/mnt/etc/udev/rules.d/90-HPE-iLO.rules
ACTION=="add", \
KERNEL=="sd*", \
ATTRS{idVendor}=="03f0", \
ATTRS{idProduct}=="2127", \
RUN+="/usr/bin/mkdir -p /media/HPE_iLO_Virtual_Folder", \
RUN+="/usr/bin/mount /dev/disk/by-id/usb-HP_Virtual_Media-0:0 /media/HPE_iLO_Virtual_Folder" 
ACTION=="remove", \
ENV{ID_VENDOR_ID}=="03f0", \
ENV{ID_MODEL_ID}=="2127", \
RUN+="/usr/bin/umount /media/HPE_iLO_Virtual_Folder", \
RUN+="/usr/bin/rmdir /media/HPE_iLO_Virtual_Folder"
ACTION=="add", \
KERNEL=="sr*", \
ATTRS{idVendor}=="03f0", \
ATTRS{idProduct}=="2027", \
RUN+="/usr/bin/mkdir -p /media/HPE_iLO_Virtual_DVD", \
RUN+="/usr/bin/mount /dev/disk/by-id/usb-HP_Virtual_DVD-ROM-0:0 /media/HPE_iLO_Virtual_DVD"
ACTION=="remove", \
ENV{ID_VENDOR_ID}=="03f0", \
ENV{ID_MODEL_ID}=="2027", \
RUN+="/usr/bin/umount /media/HPE_iLO_Virtual_DVD", \
RUN+="/usr/bin/rmdir /media/HPE_iLO_Virtual_DVD"
EOF

# Create file /etc/hpe-release
$ECHO "# Create file /etc/hpe-release"
$ECHO "SOLCOM='"$SOLCOM"'" >/mnt/etc/hpe-release
COMPONENT='/CS900_4.0/BWoH_SO/Racks/Rack01/HPE_Superdome_X_Enclosure/nPartitions/nPartition'
$ECHO "COMPONENT='"$COMPONENT"'" >>/mnt/etc/hpe-release
NPARTITION=''
$ECHO "NPARTITION='"$NPARTITION"'" >>/mnt/etc/hpe-release
MEDIUM1=$(</tmp/medium.id)
$ECHO "MEDIUM1='"$MEDIUM1"'" >>/mnt/etc/hpe-release

$ECHO "result=$result"
exit $result
        ]]></source>
      </script>
