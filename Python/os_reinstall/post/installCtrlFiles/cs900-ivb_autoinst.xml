          <entry>
            <label>Reboot system</label>
            <value>reboot</value>
          </entry>
          <entry>
            <label>Halt system</label>
            <value>halt</value>
          </entry>
        </selection>
        <default>DEFAULT</default>
        <timeout config:type="integer">0</timeout>
        <stage>initial</stage>
        <script>
          <filename>cs900-ask-pre-script-debug.sh</filename>
          <feedback config:type="boolean">true</feedback>
          <environment config:type="boolean">true</environment>
          <rerun_on_error config:type="boolean">true</rerun_on_error>
          <debug config:type="boolean">false</debug>
          <source><![CDATA[
#!/bin/bash

LOG="/tmp/cs900-autoinst-pre-script.log"

CAT=/usr/bin/cat
CP=/bin/cp
DMIDECODE=/usr/sbin/dmidecode
ECHO=/usr/bin/echo
LS=/usr/bin/ls
LSSCSI=/usr/bin/lsscsi
MULTIPATH=/sbin/multipath
SLEEP=/usr/bin/sleep

result=0
case "$VAL" in
    "display")
        set -x
        $DMIDECODE -t 1
        $LSSCSI
        $MULTIPATH -ll
        $LS -lsi /dev/mapper
        $LS -lsi /dev/disk/by-id
        $CAT $LOG
        set +x
        result=1
        ;;
    "continue")
        result=0
        ;;
    "reboot")
        $ECHO b > /proc/sysrq-trigger
        $SLEEP 10
        result=1
        ;;
    "halt")
        $ECHO o > /proc/sysrq-trigger
        $SLEEP 10
        result=1
        ;;
esac
exit $result
          ]]></source>
        </script>
